[TYPECHECK]

# List of members which are set dynamically and missed by Pylint inference
# system, and so shouldn't trigger E1101 when accessed.
generated-members=numpy.*, torch.*
good-names=i,j,df,n_*,t,n,fh,ex,da,ds,ax,lx,ly,dt,fn
disable=missing-docstring,no-member,too-many-locals

[FORMAT]
good-names=i,j,df,n_*,p,N_*,n,ds,dx,fn
